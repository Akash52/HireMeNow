    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.4.16",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.1.8",
    "vite": "^3.0.7",
    "vite-plugin-pwa": "^1.0.0"
  },
  "dependencies": {
    "driver.js": "^1.3.5",
    "highlight.js": "^11.11.1"
